local hwidtable = loadstring(game:HttpGet("https://pastebin.com/raw/ZVgx6DtV"))()
local hwid = game:GetService("RbxAnalyticsService"):GetClientId()

player = game.Players.LocalPlayer

function notify(title,text,time)
    game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = title;
    Text = text;
    Duration = time;})
end

if table.find(hwidtable, hwid) then
    notify('whitelisted', 'your whitelisted!', 4)
    wait(1.3)
    notify('script loading', 'may take couple seconds', 1)
    loadstring(game:HttpGet('https://pastebin.com/raw/uv1CRnCa'))()
else game.Players.LocalPlayer:Kick("not whitelisted") return end